<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
  <plist version="1.0">
  <dict>
    <key>Label</key>
    <string>net.peervpn.{{ item.networkname }}</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/peervpn</string>
      <string>/Library/Application Support/net.peervpn/{{ item.networkname }}.conf</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <false/>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/net.peervpn.{{ item.networkname }}.stderr.log</string>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/net.peervpn.{{ item.networkname }}.stdout.log</string>
  </dict>
</plist>
